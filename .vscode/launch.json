{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File (External Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "externalTerminal",
            // "justMyCode": false
            "subProcess": true,
        },
        {
            "name": "mFRR",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "console": "externalTerminal",
            // "justMyCode": false
            "subProcess": true,
            "args": [
                "Experiment",
                "experiment_run_mfrr",
                // "--dry-run"
                "--overwrite",
            ]
        },
        {
            "name": "FCR",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "console": "externalTerminal",
            // "justMyCode": false
            "subProcess": true,
            "args": [
                "Experiment",
                "experiment_run_fcr",
                // "--dry-run"
                "--overwrite",
            ]
        },
        {
            "name": "Streamlit (External Terminal)",
            "type": "python",
            "module": "streamlit",
            "request": "launch",
            "console": "externalTerminal",
            "args": [
                "run",
                "${file}"
            ]
        },
        {
            "name": "Build and debug active file",
            "type": "lldb",
            "request": "launch",
            "program": "${fileDirname}/${fileBasenameNoExtension}",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb",
            "preLaunchTask": "c++ build active file"
        }
    ]
}
